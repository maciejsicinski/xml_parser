            <TABLEATTRIBUTE NAME="Sql Query" VALUE="SELECT&#13;&#10;(SUM(1) OVER( ORDER BY Max_Id ROWS UNBOUNDED PRECEDING) ) + MAX_TMP.Max_Id AS Gift_Id&#13;&#10;, Src_Gift.Order_Attribute_Value AS Gift&#13;&#10;, Src_Gift.Source_Id /*Clarify source*/&#13;&#10;FROM (SELECT&#13;&#10;&#09;&#09; gift.Order_Attribute_Value,&#13;&#10;&#09;&#09; gift.Source_Id&#13;&#10;FROM (SELECT&#13;&#10;&#09; TRIM(XCA.X_Value) AS Order_Attribute_Value,&#13;&#10;&#09; 2 AS Source_Id&#13;&#10;FROM&#13;&#10;&#09;Dev_Stg.Clf_Table_Contr_Itm TCI&#13;&#10;INNER JOIN&#13;&#10;&#09;(&#13;&#10;&#09;SELECT&#13;&#10;&#09;&#09; TML.Objid&#13;&#10;&#09;&#09;,RP.Product_Id&#13;&#10;&#09;FROM&#13;&#10;&#09; Dev_Stg.Clf_Table_Mod_Level TML&#13;&#10;&#09;LEFT OUTER JOIN&#13;&#10;&#09;Dev_Stg.Vct_Rt_Product RP&#13;&#10;&#09;ON&#13;&#10;&#09;&#09;RP.Source_Key = TML.Part_Info2Part_Num&#13;&#10;&#09;AND&#13;&#10;&#09;&#09;RP.Source_Table_Name = 'Table_Part_Num'&#13;&#10;&#09;AND&#13;&#10;&#09;&#09;RP.Source_Id=2&#13;&#10;&#09;INNER JOIN&#13;&#10;&#09;&#09;Dev_Stg.Vct_Rt_Part_Number RPN&#13;&#10;&#09;ON&#13;&#10;&#09;&#09;RP.Source_Key = RPN.Part_Num_Objid&#13;&#10;&#09;AND&#13;&#10;&#09;&#09;RPN.Load_Flag =1&#13;&#10;&#09;AND&#13;&#10;&#09; &#09;RPN.Process_Group_Id IN (3,4)&#13;&#10;&#13;&#10;&#09;)TML&#13;&#10;ON&#13;&#10;&#09;TCI.Contr_Itm2Mod_Level = TML.Objid&#13;&#10;INNER JOIN&#13;&#10;&#09;Dev_Stg.Clf_Table_X_Contract_Attr XCA&#13;&#10;ON&#13;&#10; &#09;XCA.X_Contr_Attr2Contr_Itm = TCI.Objid&#13;&#10;&#13;&#10;WHERE 1=1&#13;&#10;AND CAST(TML.Product_Id AS STRING) IN (&#13;&#10;SELECT TRIM(Condition_Value) FROM Dev_Stg.Vct_Etl_Condition_Control WHERE Attribute_Name LIKE 'X_NAME_HARDWARE_PROMO_ID'  -- Hardware promotion product ids&#13;&#10;)&#13;&#10;AND XCA.X_Name IN (SELECT TRIM(Condition_Value) FROM Dev_Stg.Vct_Etl_Condition_Control WHERE Attribute_Name LIKE 'X_NAME_HARDWARE_DESC')  -- Hardware promotion descriptions&#13;&#10;&#09;Group BY 1&#13;&#10;) gift&#13;&#10;LEFT JOIN Dev_Stg.Vdw_Gifting ON Vdw_Gifting.Gift_Name = gift.Order_Attribute_Value AND Vdw_Gifting.Source_Id = gift.Source_Id&#13;&#10;&#09;WHERE Vdw_Gifting.Gift_Id IS NULL AND gift.Order_Attribute_Value &lt;&gt; 'TBD'&#13;&#10;) Src_Gift&#13;&#10;,&#13;&#10;(&#13;&#10;SELECT&#13;&#10;COALESCE(MAX(Gift_Id),0) AS Max_Id&#13;&#10;FROM Dev_Stg.Vdw_Gifting&#13;&#10;) AS MAX_TMP" />
