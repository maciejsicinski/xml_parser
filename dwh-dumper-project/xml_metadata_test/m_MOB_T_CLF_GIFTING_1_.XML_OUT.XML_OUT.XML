            <TABLEATTRIBUTE NAME="Sql Query" VALUE="-- Translation time: 2023-06-27T12:27:08.153506Z&#10;-- Translation job ID: 719ffa02-1604-47df-99f1-969724dc74e4&#10;-- Source: translation_gcp_api/dev/1687867969N-9/preprocessed/m_MOB_T_CLF_GIFTING_SQ_SC_vDW_ORDER_PRODUCT_ATTR_HIST_SourceQualifier_1.sql&#10;-- Translated from: Teradata&#10;-- Translated to: BigQuery&#10;&#10;SELECT&#10;    sum(1) OVER (ORDER BY max_tmp.max_id ROWS UNBOUNDED PRECEDING) + max_tmp.max_id AS gift_id,&#10;    src_gift.order_attribute_value AS gift_name,&#10;    src_gift.source_id&#10;  FROM&#10;    /*Clarify source*/&#10;    (&#10;      SELECT&#10;          gift.order_attribute_value,&#10;          gift.source_id&#10;        FROM&#10;          (&#10;            SELECT&#10;                trim(xca.x_value) AS order_attribute_value,&#10;                2 AS source_id&#10;              FROM&#10;                Dev_Stg.library.clf_table_contr_itm AS tci&#10;                INNER JOIN (&#10;                  SELECT&#10;                      tml.objid,&#10;                      rp.product_id&#10;                    FROM&#10;                      Dev_Stg.library.clf_table_mod_level AS tml&#10;                      LEFT OUTER JOIN Dev_Stg.library.vct_rt_product AS rp ON rp.source_key = tml.part_info2part_num&#10;                       AND upper(rp.source_table_name) = 'TABLE_PART_NUM'&#10;                       AND rp.source_id = 2&#10;                      INNER JOIN Dev_Stg.library.vct_rt_part_number AS rpn ON rp.source_key = rpn.part_num_objid&#10;                       AND rpn.load_flag = 1&#10;                       AND rpn.process_group_id IN(&#10;                        3, 4&#10;                      )&#10;                ) AS tml ON tci.contr_itm2mod_level = tml.objid&#10;                INNER JOIN Dev_Stg.library.clf_table_x_contract_attr AS xca ON xca.x_contr_attr2contr_itm = tci.objid&#10;              WHERE 1 = 1&#10;               AND tml.product_id IN(&#10;                SELECT&#10;                    trim(condition_value)&#10;                  FROM&#10;                    Dev_Stg.library.vct_etl_condition_control&#10;                  WHERE upper(attribute_name) LIKE 'X_NAME_HARDWARE_PROMO_ID'&#10;              )&#10;               AND xca.x_name IN(&#10;                SELECT&#10;                    --  Hardware promotion product ids&#10;                    trim(condition_value)&#10;                  FROM&#10;                    Dev_Stg.library.vct_etl_condition_control&#10;                  WHERE upper(attribute_name) LIKE 'X_NAME_HARDWARE_DESC'&#10;              )&#10;              GROUP BY 1&#10;          ) AS gift&#10;          LEFT OUTER JOIN --  Hardware promotion descriptions&#10;          Dev_Stg.library.vdw_gifting ON vdw_gifting.gift_name = gift.order_attribute_value&#10;           AND vdw_gifting.source_id = gift.source_id&#10;        WHERE vdw_gifting.gift_id IS NULL&#10;         AND gift.order_attribute_value &lt;&gt; 'TBD'&#10;    ) AS src_gift&#10;    CROSS JOIN (&#10;      SELECT&#10;          coalesce(max(vdw_gifting.gift_id), CAST(0 as BIGNUMERIC)) AS max_id&#10;        FROM&#10;          Dev_Stg.library.vdw_gifting&#10;    ) AS max_tmp&#10;;&#10;" />
